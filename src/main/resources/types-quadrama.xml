		<typeDescription>
			<name>de.unistuttgart.ims.drama.api.Figure</name>
			<description>Represents one figure in the play. Each figure should
				only have one Figure annotation (and multiple Speaker annotations)</description>
			<supertypeName>uima.tcas.Annotation</supertypeName>
			<features>
				<featureDescription>
					<name>Id</name>
					<description/>
					<rangeTypeName>uima.cas.Integer</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>XmlId</name>
					<description/>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>Description</name>
					<description/>
